    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.2.1",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "angular-http-server": "^1.11.1",
    "child_process": "^1.0.2",
    "chrome-launcher": "^0.15.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.23.1",
    "jasmine-core": "~4.3.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
