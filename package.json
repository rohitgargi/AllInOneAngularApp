{
  "name": "angular-poc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "webserver": "angular-http-server --silent -p 9000 --path ./dist/angular-poc . &",
    "lighthouse:prod": "ng build --base-href ./ && npm run webserver && node lighthouse.js  -build",
    "lighthouse:dev": "node lighthouse.js -dev",
    "server": "node server/server.js",
    "lint": "ng lint",
    "test:accessability": "pa11y-ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
